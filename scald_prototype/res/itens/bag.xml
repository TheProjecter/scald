<!--

 File that describes the bag used, one or many
 
-->

<Bags>

	<!--A bag, all the characters have the same
	type of bag -->
	<Bag>
		<!-- If is a bag for a character or the group -->
		<Type></Type>
		<!-- The maximum quantity of slots,places that hold itens,
		one item equals one slot, that has, zero means MANY itens -->
		<MaximumSlots></MaximumSlots>	
	</Bag>
	
	<!-- Example Group -->
	<!--A bag, all the characters have the same
	type of bag -->
	<Bag>
		<!-- If is a bag for a character or the group -->
		<Type>Group</Type>
		<!-- The maximum quantity of slots,places that hold itens,
		one item equals one slot, that has, zero means MANY itens -->
		<MaximumSlots>0</MaximumSlots>	
	</Bag>
	
	<!-- Example Character -->
	<!--A bag, all the characters have the same
	type of bag -->
	<Bag>
		<!-- If is a bag for a character or the group -->
		<Type>Character</Type>
		<!-- The maximum quantity of slots,places that hold itens,
		one item equals one slot, that has, zero means MANY itens -->
		<MaximumSlots>15</MaximumSlots>	
	</Bag>
	
</Bags>
