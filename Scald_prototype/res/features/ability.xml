	<!-- The identifier of the skill -->
	<HitPoints>
		<!-- Has the general information -->
		<General>
			<!-- Name, will be used in the game -->
			<Name>HP</Name>			
			<!-- General type -->
			<Type>Skill</Type>		
			<!-- Description -->
			<Description>The hit points of the character</Description>			
		</General>		
		
		<!--more specific type-->
		<SpecificType>
			<!-- the others type -->
			<!-- type -->
			<TypeOf>Status</TypeOf>					
		</SpecificType>
		
		<!-- Has the values, the initial and the maximum value, that can have,
		to be used for things like strenght, agility, isn´t used as a level -->
		<Values>
			<!-- Initial value -->
			<Initial>10</Initial>
			<!-- Maximum value can have -->
			<Maximum>9999</Maximum>
		</Values>
		
	</HitPoints>

